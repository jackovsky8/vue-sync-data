{
    "root": true,
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "globals": {
      "ENV": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "vue",
      "prettier"
    ],
    "parserOptions": {
      "sourceType": "module",
      "parser": "babel-eslint"
    }
  }